{
    "source": {
        "include": ["./src/"],
        "includePattern": ".+\\.mjs"
    },
    "opts": {
        "destination": "./doc/"
    },
    "templates": {
        "default": {
            "includeDate": false
        }
    },
    "plugins": [
      "plugins/markdown"
    ],
    "markdown": {
        "tags": ["description", "param"],
        "hardwrap": false
    }
}
